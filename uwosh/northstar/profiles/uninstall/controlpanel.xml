<?xml version="1.0"?>
<object name="portal_controlpanel" 
        meta_type="Plone Control Panel Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n" 
        i18n:domain="uwosh.simpleemergency">

	<configlet title="Workflow Manager" action_id="uwosh-northstar"
        appId="uwosh.northstar" category="Products" condition_expr=""
        url_expr="string:${portal_url}/@@uwosh-northstar"
        visible="True" i18n:attributes="title"
        remove="True">
        <permission>Manage portal</permission>
    </configlet>
    <configlet title="Workflow App Generator" action_id="uwosh-northstar-app-generator"
      appId="uwosh.northstar-app-generator" category="Products" condition_expr=""
      url_expr="string:${portal_url}/@@uwosh-northstar-app-generator"
      visible="True" i18n:attributes="title" remove="True">
      <permission>Manage portal</permission>
    </configlet>
</object>